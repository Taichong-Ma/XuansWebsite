// Typography: Headings Code
#typography-headings-code.modal.fade
  .modal-dialog.modal-lg
    .modal-content
      .modal-header
        h4.modal-title Code
        button(type='button' class='close' data-dismiss='modal' aria-label='Close')
          span(aria-hidden='true') &times;
      .modal-body.pb-0
        ul(class='nav nav-tabs pre-nav-tabs justify-content-end' role='tablist')
          li.nav-item
            a(href='#html1' class='nav-link active' data-toggle='tab' role='tab') HTML
          li.nav-item
            a(href='#pug1' class='nav-link' data-toggle='tab' role='tab') Pug
        .tab-content
          div(id='html1' class='tab-pane fade show active' role='tabpanel')
            pre.text-md
              code.
                <strong>&lt;!-- Typography: Headings H1 - H6 --&gt;</strong>
                &lt;h1&gt;H1. Heading&lt;/h1&gt;
                &lt;h2&gt;H2. Heading&lt;/h2&gt;
                &lt;h3&gt;H3. Heading&lt;/h3&gt;
                &lt;h4&gt;H4. Heading&lt;/h4&gt;
                &lt;h5&gt;H5. Heading&lt;/h5&gt;
                &lt;h6&gt;H6. Heading&lt;/h6&gt;

                <strong>&lt;!-- Via Class --&gt;</strong>
                &lt;div class=&quot;h1&quot;&gt;H1. Heading&lt;/div&gt;
                &lt;div class=&quot;h2&quot;&gt;H2. Heading&lt;/div&gt;
                &lt;div class=&quot;h3&quot;&gt;H3. Heading&lt;/div&gt;
                &lt;div class=&quot;h4&quot;&gt;H4. Heading&lt;/div&gt;
                &lt;div class=&quot;h5&quot;&gt;H5. Heading&lt;/div&gt;
                &lt;div class=&quot;h6&quot;&gt;H6. Heading&lt;/div&gt;
          div(id='pug1' class='tab-pane fade' role='tabpanel')
            pre.text-md
              code.
                <strong>//- Typography: Headings H1 - H6</strong>
                h1 H1. Heading
                h2 H2. Heading
                h3 H3. Heading
                h4 H4. Heading
                h5 H5. Heading
                h6 H6. Heading

                <strong>//- Via Class</strong>
                .h1 H1. Heading
                .h2 H2. Heading
                .h3 H3. Heading
                .h4 H4. Heading
                .h5 H5. Heading
                .h6 H6. Heading
