  //- Progress Style 1 (Default) Code Modal
  include code/progress-style1

  //- Progress Style 2 Code Modal
  include code/progress-style2

  //- Progress Style 3 Code Modal
  include code/progress-style3

  //- Progress Animated When in View Code Modal
  include code/progress-animated

  // Off-Canvas (Sidebar)
  include ../partials/sidebar-components

  // Off-Canvas Wrapper
